{
  "name": "ai-chatbot-assistant-backend",
  "version": "1.0.0",
  "description": "AI Chatbot Assistant backend with Cohere integration",
  "main": "dist/api/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/api/server.ts",
    "build": "tsc",
    "start": "node dist/api/server.js",
    "test": "jest",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:coverage": "jest --coverage",
    "test:conversation": "ts-node scripts/test-conversation.ts",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "ai",
    "chatbot",
    "cohere",
    "agent",
    "conversational-ai"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cohere-ai": "^7.7.0",
    "express": "^4.18.2",
    "zod": "^3.22.4",
    "uuid": "^9.0.1",
    "dotenv": "^16.4.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.16",
    "@types/uuid": "^9.0.7",
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.12",
    "@types/supertest": "^6.0.2",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "supertest": "^6.3.4",
    "axios": "^1.6.7",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "prettier": "^3.2.5"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
